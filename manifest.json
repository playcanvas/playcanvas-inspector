{
  "name": "PlayCanvas Inspector",
  "version": "1.0",
  "description": "Inspect published PlayCanvas applications",
  "manifest_version": 2,
  "minimum_chrome_version": "83.0",
  "devtools_page": "devtools.html",
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "web_accessible_resources": [
    "playcanvas-devtools/injector.js",
    "playcanvas-devtools/pc-devtools.js",
    "playcanvas-devtools/dat.gui.min.js",
    "playcanvas-devtools/debug-physics.js",
    "playcanvas-devtools/playcanvas-extras.js"
  ]
}
